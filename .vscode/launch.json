            "name": "Debug MyNeuralNetworks",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/debug/MyNeuralNetworksDebug",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "preLaunchTask": "Build (Debug)",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
